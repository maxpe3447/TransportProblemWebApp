@model InformationField
@{
    TextField textField = ViewBag.TextField;

    ViewBag.Title = textField.Title;
    ViewBag.Description = textField.MetaDescription;
}
<div>
    <h2>
        @Model.Title
    </h2>
    <h3>
        @Model.Subtitle
    </h3>
    <div>
        <img class="img-block" src="~/images/@Model.TitleImagePath"/>
    </div>
</div>
<div>
    @Html.Raw(Model.Text)
</div>